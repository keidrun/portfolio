{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Keid's portfolio site.",
  "engines": {
    "node": ">= 14.0.0"
  },
  "author": "keidrun",
  "license": "GPL-3.0-only",
  "private": true,
  "keywords": [],
  "dependencies": {
    "bootstrap": "^5.1.3",
    "fs-extra": "^10.1.0",
    "gatsby": "^4.17.1",
    "gatsby-plugin-google-analytics": "^4.12.1",
    "gatsby-plugin-graphql-codegen": "^3.1.1",
    "gatsby-plugin-image": "^2.17.0",
    "gatsby-plugin-manifest": "^4.12.1",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sass": "^5.12.1",
    "gatsby-plugin-sharp": "^4.17.0",
    "gatsby-source-filesystem": "^4.17.0",
    "gatsby-transformer-sharp": "^4.17.0",
    "glob": "^8.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^1.3.0",
    "reactstrap": "8.10.1",
    "reactstrap-scrollspy": "^0.1.1",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet-async": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gatsby-plugin-eslint": "^4.0.2",
    "hermes-eslint": "^0.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.8.1",
    "typed-scss-modules": "^6.5.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.s?css": [
      "yarn stylelint:fix",
      "yarn prettier:write"
    ],
    "src/**/*.{jsx?,tsx?}": [
      "yarn type-check",
      "yarn eslint:fix",
      "yarn prettier:write"
    ]
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "prettier:check": "yarn prettier --check",
    "prettier:write": "yarn prettier --write",
    "eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "yarn eslint --fix",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "yarn stylelint --fix",
    "typed-scss-modules": "typed-scss-modules src --outputFolder types/__generated__",
    "type-check": "tsc --noEmit",
    "preoptimize-images": "ts-node scripts/optimize.ts",
    "ci:lint": "yarn type-check && yarn eslint && yarn stylelint && yarn prettier:check"
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
