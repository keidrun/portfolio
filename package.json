{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Keid's portfolio site.",
  "engines": {
    "node": ">= 12.0.0"
  },
  "author": "keidrun",
  "license": "GPL-3.0-only",
  "private": true,
  "keywords": [],
  "dependencies": {
    "bootstrap": "^4.4.1",
    "gatsby": "^2.18.18",
    "gatsby-plugin-flow": "^1.1.17",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.26",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "reactstrap": "^8.2.0",
    "reactstrap-scrollspy": "^0.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "css-url-loader": "^2.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "flow-bin": "^0.115.0",
    "flow-typed": "^2.6.2",
    "husky": "^4.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn stylelint && yarn flow-check && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn eslint",
      "git add"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "eslint": "eslint --fix src/**/*.js",
    "stylelint": "stylelint --fix src/**/*.scss",
    "flow-check": "flow check",
    "flow-typed-install": "flow-typed install",
    "aws:deploy": "aws s3 sync ./public s3://keidrun-portfolio-website",
    "aws:clear-cache": "aws cloudfront create-invalidation --distribution-id E38TFPFDJEAZGV --paths '/index.html'",
    "aws:clear-cache-all": "aws cloudfront create-invalidation --distribution-id E38TFPFDJEAZGV --paths '/*'"
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ]
}
