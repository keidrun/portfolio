{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Keid's portfolio site.",
  "engines": {
    "node": ">= 12.00.0"
  },
  "author": "keidrun",
  "license": "GPL-3.0-only",
  "private": true,
  "keywords": [],
  "dependencies": {
    "bootstrap": "^5.1.3",
    "gatsby": "^4.4.0",
    "gatsby-plugin-flow": "^3.4.0",
    "gatsby-plugin-google-analytics": "^4.4.0",
    "gatsby-plugin-manifest": "^4.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-robots-txt": "^1.6.14",
    "gatsby-plugin-sass": "^5.4.0",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "reactstrap": "^9.0.1",
    "reactstrap-scrollspy": "^0.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "flow-bin": "^0.168.0",
    "flow-typed": "^3.6.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "prettier": "^2.5.1",
    "sass-loader": "^12.4.0",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{s?css}": [
      "yarn stylelint"
    ],
    "src/**/*.{jsx?}": [
      "yarn flow-check",
      "yarn eslint"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "eslint": "eslint --fix src/**/*.js",
    "stylelint": "stylelint --fix src/**/*.scss",
    "flow-check": "flow check",
    "flow-typed-install": "flow-typed install"
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
